<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<TabPane fx:id="tabPane" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <Tab fx:id="backgroundsTab" text="Backgrounds">
        <FlowPane fx:id="backgroundsFlowPane" prefHeight="600.0" prefWidth="1200.0">
            <VBox fx:id="backgroundCard1" prefHeight="200.0" prefWidth="200.0">
                <ImageView fx:id="backgroundImageView" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="backgroundLabel1" text="Background 1" />
                <Button fx:id="backgroundBuyButton1" mnemonicParsing="false" text="Buy ($5)" />
            </VBox>
            <VBox fx:id="backgroundCard2" prefHeight="200.0" prefWidth="200.0">
                <ImageView fx:id="backgroundImageView1" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="backgroundLabel2" text="Background 2" />
                <Button fx:id="backgroundBuyButton2" mnemonicParsing="false" text="Buy ($10)" />
            </VBox>
        </FlowPane>
    </Tab>
    <Tab fx:id="rocketsTab" text="Rockets">
        <FlowPane fx:id="rocketsFlowPane" prefHeight="600.0" prefWidth="1200.0">
            <VBox fx:id="rocketCard1" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
                <ImageView fx:id="rocketImageView1" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="rocketLabel1" text="Rocket 1" />
                <Button fx:id="rocketBuyButton1" mnemonicParsing="false" text="Buy ($20)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <padding>
               <Insets bottom="3.0" top="3.0" />
            </padding>
            </VBox>
            <VBox fx:id="rocketCard2" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
                <ImageView fx:id="rocketImageView2" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="rocketLabel2" text="Rocket 2" />
                <Button fx:id="rocketBuyButton2" mnemonicParsing="false" text="Buy ($30)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <padding>
               <Insets bottom="3.0" top="3.0" />
            </padding>
            </VBox>
            <VBox fx:id="rocketCard3" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
            <ImageView fx:id="rocketImageView3" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="rocketLabel3" text="Rocket 3" />
            <Button fx:id="rocketBuyButton3" mnemonicParsing="false" text="Buy ($30)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <padding>
               <Insets bottom="3.0" top="3.0" />
            </padding>
        </VBox>
            <VBox fx:id="rocketCard4" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
            <ImageView fx:id="rocketImageView4" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="rocketLabel4" text="Rocket 4" />
            <Button fx:id="rocketBuyButton4" mnemonicParsing="false" text="Buy ($30)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <padding>
               <Insets bottom="3.0" top="3.0" />
            </padding>
        </VBox>
            <VBox fx:id="rocketCard5" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
            <ImageView fx:id="rocketImageView5" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="rocketLabel5" text="Rocket 5" />
            <Button fx:id="rocketBuyButton5" mnemonicParsing="false" text="Buy ($30)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <padding>
               <Insets bottom="3.0" top="3.0" />
            </padding>
        </VBox>
        </FlowPane>
    </Tab>
</TabPane>
